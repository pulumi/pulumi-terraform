{
  "name": "terraform",
  "displayName": "Terraform",
  "description": "The Terraform provider for Pulumi lets you consume the outputs\ncontained in Terraform state files from your Pulumi programs.\n\nThere is one function per kind of Terraform state backend to consume state from.",
  "keywords": [
    "terraform",
    "kind/native",
    "category/utility"
  ],
  "homepage": "https://pulumi.com",
  "license": "Apache-2.0",
  "repository": "https://github.com/pulumi/pulumi-terraform",
  "logoUrl": "https://raw.githubusercontent.com/pulumi/pulumi-terraform-provider/main/assets/logo.png",
  "publisher": "Pulumi",
  "meta": {
    "moduleFormat": "(.*)"
  },
  "language": {
    "csharp": {
      "packageReferences": {
        "Pulumi": "3.*"
      },
      "respectSchemaVersion": true
    },
    "go": {
      "generateResourceContainerTypes": true,
      "importBasePath": "github.com/pulumi/pulumi-terraform/sdk/go/terraform",
      "respectSchemaVersion": true
    },
    "java": {
      "buildFiles": "gradle",
      "dependencies": {
        "com.google.code.findbugs:jsr305": "3.0.2",
        "com.google.code.gson:gson": "2.8.9",
        "com.pulumi:pulumi": "1.0.0"
      },
      "gradleNexusPublishPluginVersion": "2.0.0"
    },
    "nodejs": {
      "respectSchemaVersion": true
    },
    "python": {
      "pyproject": {
        "enabled": true
      },
      "respectSchemaVersion": true
    }
  },
  "config": {},
  "types": {
    "terraform:index:Workspaces": {
      "properties": {
        "name": {
          "type": "string"
        },
        "prefix": {
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "provider": {
    "type": "object"
  },
  "functions": {
    "terraform:index:localStateReference": {
      "description": "TODO",
      "inputs": {
        "properties": {
          "path": {
            "type": "string"
          }
        },
        "type": "object",
        "required": [
          "path"
        ]
      },
      "outputs": {
        "description": "The result of fetching from a Terraform state store.",
        "properties": {
          "outputs": {
            "additionalProperties": {
              "$ref": "pulumi.json#/Any"
            },
            "description": "The outputs displayed from Terraform state.",
            "type": "object"
          }
        },
        "required": [
          "outputs"
        ],
        "type": "object"
      }
    },
    "terraform:index:remoteStateReference": {
      "description": "TODO",
      "inputs": {
        "properties": {
          "hostname": {
            "type": "string"
          },
          "organization": {
            "type": "string"
          },
          "token": {
            "type": "string"
          },
          "workspaces": {
            "$ref": "#/types/terraform:index:Workspaces"
          }
        },
        "type": "object",
        "required": [
          "organization",
          "workspaces"
        ]
      },
      "outputs": {
        "description": "The result of fetching from a Terraform state store.",
        "properties": {
          "outputs": {
            "additionalProperties": {
              "$ref": "pulumi.json#/Any"
            },
            "description": "The outputs displayed from Terraform state.",
            "type": "object"
          }
        },
        "required": [
          "outputs"
        ],
        "type": "object"
      }
    }
  }
}
